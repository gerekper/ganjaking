#, php-format
msgid "%1$s: Day of the month, for example 28."
msgstr "%1$s : jour du mois, par exemple 28."

#: /home/tony/websites/www-dev/public/wp_plugins/wp-sitemap-page/plugin_1_5_4_next/settings.php:122
#, php-format
msgid "%1$s: Hour of the day, for example 15."
msgstr "%1$s : heure du jour de publication, par exemple 15."

#: /home/tony/websites/www-dev/public/wp_plugins/wp-sitemap-page/plugin_1_5_4_next/settings.php:123
#, php-format
