#, php-format
msgid "%1$s: URL of the post."
msgstr "%1$s: bejegyzés URL."

#: /home/tony/websites/www-dev/public/wp_plugins/wp-sitemap-page/plugin_1_5_4_next/settings.php:119
#, php-format
msgid "%1$s: The year of the post, four digits, for example 2004."
msgstr "%1$s: A bejegyzés dátumának évszáma négy számjeggyel, pl. 2004."

#: /home/tony/websites/www-dev/public/wp_plugins/wp-sitemap-page/plugin_1_5_4_next/settings.php:120
#, php-format
msgid "%1$s: Month of the year, for example 05."
msgstr "%1$s: Az év hányadik hónapja, pl. 05."

#: /home/tony/websites/www-dev/public/wp_plugins/wp-sitemap-page/plugin_1_5_4_next/settings.php:121
#, php-format
msgid "%1$s: Day of the month, for example 28."
msgstr "%1$s: A hónap napja, pl. 05."

#: /home/tony/websites/www-dev/public/wp_plugins/wp-sitemap-page/plugin_1_5_4_next/settings.php:122
#, php-format
msgid "%1$s: Hour of the day, for example 15."
msgstr "%1$s: Hányadik óra a napban, pl. 15."

